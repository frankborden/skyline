{
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier -w ."
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "latest",
    "@fontsource-variable/fira-code": "latest",
    "@fontsource-variable/inter": "latest",
    "@iconify/json": "latest",
    "@react-aria/toast": "latest",
    "@react-stately/toast": "latest",
    "@tanstack/react-router": "latest",
    "@tanstack/router-vite-plugin": "latest",
    "@trivago/prettier-plugin-sort-imports": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@vitejs/plugin-react-swc": "latest",
    "autoprefixer": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "react": "latest",
    "react-aria-components": "latest",
    "react-dom": "latest",
    "tailwind-merge": "latest",
    "tailwind-variants": "latest",
    "tailwindcss": "latest",
    "tailwindcss-animate": "latest",
    "tailwindcss-react-aria-components": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "latest"
  },
  "prettier": {
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
      "^~/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
